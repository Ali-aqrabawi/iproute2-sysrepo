<links xmlns="urn:okda:iproute2:ip:link">
    <link>
        <name>if_tc_f1</name>
        <type>dummy</type>
        <admin-status>up</admin-status>
    </link>
    <link>
        <name>if_tc_f2</name>
        <type>dummy</type>
        <admin-status>up</admin-status>
    </link>
    <link>
        <name>if_tc_f3</name>
        <type>dummy</type>
        <admin-status>up</admin-status>
