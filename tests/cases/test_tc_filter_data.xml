        <htb>
            <rate>20mbit</rate>
        </htb>
    </class>
</classes>

<qdiscs xmlns="urn:okda:iproute2:tc:qdisc">
    <qdisc>
        <dev>if_tc_f1</dev>
        <parent>root</parent>
        <handle>8001:</handle>
        <qdisc-kind>htb</qdisc-kind>
    </qdisc>
    <qdisc>
        <dev>if_tc_f1</dev>
        <parent>8001:20</parent>
        <qdisc-kind>pfifo</qdisc-kind>
        <fifo-options>
            <limit>1002</limit>
        </fifo-options>
    </qdisc>
    <qdisc>
        <dev>if_tc_f1</dev>
        <parent>8001:30</parent>
        <qdisc-kind>pfifo</qdisc-kind>
        <fifo-options>
            <limit>1003</limit>
        </fifo-options>
    </qdisc>
    <qdisc>
        <dev>if_tc_f1</dev>
        <parent>8001:40</parent>
        <qdisc-kind>pfifo</qdisc-kind>
        <fifo-options>
            <limit>1004</limit>
        </fifo-options>
    </qdisc>
    <qdisc>
        <dev>if_tc_f2</dev>
        <parent>root</parent>
        <handle>8002:</handle>
        <qdisc-kind>htb</qdisc-kind>
    </qdisc>
    <special-qdisc>
        <dev>if_tc_f3</dev>
        <qdisc-kind>clsact</qdisc-kind>
        <ingress_block>10</ingress_block>
    </special-qdisc>
    <special-qdisc>
        <dev>if_tc_f4</dev>
        <qdisc-kind>ingress</qdisc-kind>
    </special-qdisc>
</qdiscs>
